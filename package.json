{
  "name": "offline-wallet-generator",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "parcel:build": "parcel build",
    "parcel:watch": "parcel watch",
    "parcel:serve": "parcel serve src/main.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && cp -r node_modules/@theqrl/qrllib-browserify/dist/qrllib.js dist/js/qrllib.js && node post-build.js",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "start": "webpack-dev-server --progress --colors --inline --content-base build",
    "semantic-release": "semantic-release",
    "preinstall": "npx npm-force-resolutions",
    "postinstall": "cp -r node_modules/@theqrl/qrllib-browserify/dist/qrllib.js public/qrllib.js",
    "prepare": "npm run snyk-protect",
    "snyk-protect": "snyk-protect"
  },
  "dependencies": {
    "@babel/core": "^7.18.6",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "@popperjs/core": "^2.11.5",
    "@theqrl/qrllib-browserify": "^1.0.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
    "@vue/babel-preset-jsx": "^1.2.4",
    "@vue/compat": "^3.2.37",
    "aes256": "^1.1.0",
    "bech32": "^2.0.0",
    "bootstrap": "^5.1.3",
    "browserify-fs": "^1.0.0",
    "buffer": "^6.0.3",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.0",
    "hack-font": "^3.3.0",
    "html2pdf": "0.0.11",
    "html2pdf.js": "^0.10.1",
    "jquery": "^3.6.0",
    "jspdf": "^2.5.1",
    "path-browserify": "^1.0.1",
    "print-js": "^1.6.0",
    "roboto-fontface": "^0.10.0",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.4",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16",
    "vuex": "^4.0.2",
    "@snyk/protect": "latest"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.6.2",
    "@parcel/transformer-vue": "^2.6.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@vue/cli-plugin-babel": "^5.0.6",
    "@vue/cli-plugin-router": "^5.0.6",
    "@vue/cli-service": "^5.0.6",
    "@vue/compiler-sfc": "^3.2.37",
    "bl": "^5.0.0",
    "html-webpack-inline-source-plugin": "^1.0.0-beta.2",
    "html-webpack-plugin": "^5.5.0",
    "parcel": "^2.6.2",
    "parcel-resolver-ignore": "^2.1.1",
    "semantic-release": "^19.0.3",
    "semver": "^7.3.7",
    "vue-template-compiler": "^2.6.14"
  },
  "resolutions": {
    "semver": "7.3.7",
    "bl": "5.0.0"
  },
  "parcelIgnore": [
    "qrllib.js"
  ],
  "snyk": true
}
